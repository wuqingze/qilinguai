#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_node-static@0.7.10@node-static/bin/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../_node-static@0.7.10@node-static/bin/cli.js" "$@"
  ret=$?
fi
exit $ret
